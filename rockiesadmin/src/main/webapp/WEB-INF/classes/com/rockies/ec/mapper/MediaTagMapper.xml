<?xml version="1.0" encoding="UTF-8" ?>
	<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.rockies.ec.mapper.MediaTagMapper">
    
    <!-- 添加 返回主键id -->
    <insert id="insert" parameterType="com.rockies.ec.model.MediaTag" useGeneratedKeys="true" keyProperty="id">
    	INSERT INTO mediatag(name,mediaId,ctdt,ctuser,updt,upuser) 
    	values(#{name},#{mediaId},#{ctdt},#{ctuser},#{updt},#{upuser})
    	
   		
    </insert>
    
    <!-- 根据主键删除 -->
    <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer" >
    	delete from mediatag where id=#{id}
    </delete>
    
    <!-- 获取对象集合 -->
    <select id="getAllList" resultType="com.rockies.ec.model.MediaTag">
    	select * from mediatag
    </select>
    
    <!-- 获取对象集合 parameterType="Map" 选择是否分页-->
    <select id="getAllListByMap" resultType="com.rockies.ec.model.MediaTag" parameterType="Map">
    	select * from mediatag
    	<where>
    		1=1
    		<if test="name!=null">
		  		and name like "%${name}%" 
			</if>
			<if test="mediaId!=null and mediaId!=''">
		  		and mediaId=#{mediaId}
			</if>
			<if test="ctdt!=null and ctdd!=''">
				and ctdt=#{ctdt}
			</if>
			<if test="ctuser!=null and ctuser!=''">
				and ctuser=#{ctuser}
			</if>
			<if test="upuser!=null">
				and upuser=#{upuser}
			</if>
			<if test="updt!=null and updt!=''">
				and updt=#{updt}
			</if>
    	</where>
		<if test="offset!=null and limit!=null">
			limit #{offset},#{limit}
		</if>
    </select>
    
    
    <!-- 获取map集合  parameterType="Map" 分页-->
    <select id="getAllListMap" resultType="Map" parameterType="Map">
    	select * from mediatag
    	<where>
    		1=1
    		<if test="name!=null">
		  		and name like "%${name}%" 
			</if>
			<if test="mediaId!=null and mediaId!=''">
		  		and mediaId=#{mediaId}
			</if>
			<if test="ctdt!=null and ctdt!=''">
				and ctdt=#{ctdt}
			</if>
			<if test="ctuser!=null and ctuser!=''">
				and ctuser=#{ctuser}
			</if>
			<if test="upuser!=null and upuser!=''">
				and upuser=#{upuser}
			</if>
			<if test="updt!=null and updt!=''">
				and updt=#{updt}
			</if>
    	</where>
		<if test="offset!=null and limit!=null">
			limit #{offset},#{limit}
		</if>
    </select>
    
    <!-- 根据主键获取一个对象-->
    <select id="getOneByPrimaryKey" resultType="com.rockies.ec.model.MediaTag" parameterType="java.lang.Integer">
    	select * from mediatag where id=#{id}
    </select>
    
    <!-- 总记录数-->
    <select id="getCount" resultType="java.lang.Long" parameterType="Map">
    	select count(*) from mediatag 
    	<where>
    		1=1
    		<if test="name!=null">
		  		and name like "%${name}%" 
			</if>
			<if test="mediaId!=null">
		  		and mediaId=#{mediaId}
			</if>
			<if test="ctdt!=null and ctdt!=''">
				and ctdt=#{ctdt}
			</if>
			<if test="ctuser!=null and ctuser!=''">
				and ctuser=#{ctuser}
			</if>
			<if test="upuser!=null and upuser!=''">
				and upuser=#{upuser}
			</if>
			<if test="updt!=null and updt!=''">
				and updt=#{updt}
			</if>
    	</where>
    </select>
    
    <update id="updateObject"  parameterType="Map">
    	update mediatag 
		<set>
			<if test="name!=null and name!=''">
		  		name=#{name},
			</if>
			<if test="mediaId!=null and mediaId!=''">
		  		mediaId=#{mediaId},
			</if>
			<if test="ctdt!=null">
				ctdt=#{ctdt},
			</if>
			<if test="ctuser!=null">
				ctuser=#{ctuser},
			</if>
			<if test="upuser!=null">
				upuser=#{upuser},
			</if>
			<if test="updt!=null">
				updt=#{updt}
			</if>
		</set>
		where id=#{id}
    </update>
    
    
</mapper>